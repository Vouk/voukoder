{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "mp4",
  "name": "MPEG-4 (.mp4)",
  "extension": "mp4",
  "encoders": {
    "video": [ "h264_amf", "h264_nvenc", "hevc_amf", "hevc_nvenc", "libx264", "libx265", "libvpx", "libvpx-vp9" ],
    "audio": [ "aac", "libfdk_aac", "ac3", "eac3", "libmp3lame" ]
  },
  "capabilities": {
    "faststart": true
  }
}
