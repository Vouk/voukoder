{
  "name": "libvpx",
  "authors": [
    "Daniel Stankewitz"
  ],
  "defaultPixelFormat": "yuv420p",
  "multipass": {
    "name": "vp8Passes"
  },
  "params": [
    {
      "name": "vp8Threads",
      "label": "Threads",
      "description": "Threads to use for parallel encoding",
      "group": "ADBEVideoCodecGroup",
      "type": "int",
      "flags": [ "slider" ],
      "defaultValue": 8,
      "minValue": 1,
      "maxValue": 24,
      "parameters": {
        "threads": "%d"
      }
    },
    {
      "name": "vp8Strategy",
      "label": "Strategy",
      "description": "The encoding strategy to use.",
      "group": "ADBEVideoCodecGroup",
      "type": "int",
      "flags": [],
      "defaultValue": 1,
      "useDefaultValue": true,
      "values": [
        {
          "id": 0,
          "name": "Average Bitrate (ABR)",
          "subvalues": [
            {
              "name": "vp8StrategyABR",
              "label": "Target Bitrate [kbit]",
              "description": "The target data rate allowed by the encoder.",
              "type": "int",
              "flags": [ "slider" ],
              "defaultValue": 15000,
              "useDefaultValue": true,
              "minValue": 100,
              "maxValue": 100000,
              "parameters": {
                "b": "%d000"
              }
            },
            {
              "name": "vp8Passes",
              "label": "Passes",
              "description": "Number of encoding passes.",
              "type": "int",
              "flags": [ "slider" ],
              "defaultValue": 1,
              "minValue": 1,
              "maxValue": 2,
              "parameters": {}
            }
          ]
        },
        {
          "id": 1,
          "name": "Constant Quality (CRF)",
          "subvalues": [
            {
              "name": "vp8StrategyCRF",
              "label": "Constant Rate Factor",
              "description": "Lower values mean better quality but increase the processing time.",
              "type": "int",
              "flags": [ "slider" ],
              "useDefaultValue": true,
              "defaultValue": 10,
              "minValue": 4,
              "maxValue": 63,
              "parameters": {
                "crf": "%d",
                "b": "0"
              }
            }
          ]
        },
        {
          "id": 2,
          "name": "Constrained Quality (CQ)",
          "subvalues": [
            {
              "name": "vp8StrategyCQ",
              "label": "Target Bitrate [kbit]",
              "description": "The target data rate allowed by the encoder.",
              "type": "int",
              "flags": [ "slider" ],
              "defaultValue": 15000,
              "useDefaultValue": true,
              "minValue": 100,
              "maxValue": 100000,
              "parameters": {
                "b": "%d000"
              }
            },
            {
              "name": "vp8StrategyCQ2",
              "label": "Constant Rate Factor",
              "description": "Lower values mean better quality but increase the processing time.",
              "type": "int",
              "flags": [ "slider" ],
              "useDefaultValue": true,
              "defaultValue": 10,
              "minValue": 4,
              "maxValue": 63,
              "parameters": {
                "crf": "%d"
              }
            }
          ]
        }
      ]
    },
    {
      "name": "vp8Quality",
      "label": "Quality",
      "description": "Good is the default and recommended for most applications.\n\nBest is recommended if you have lots of time and want the best compression efficiency.\n\nRealtime is recommended for live / fast encoding.",
      "group": "ADBEVideoCodecGroup",
      "type": "int",
      "flags": [],
      "defaultValue": 0,
      "useDefaultValue": true,
      "values": [
        {
          "id": 0,
          "name": "Good",
          "parameters": {
            "quality": "good"
          }
        },
        {
          "id": 1,
          "name": "Best",
          "parameters": {
            "quality": "best"
          }
        },
        {
          "id": 2,
          "name": "Realtime",
          "parameters": {
            "quality": "realtime"
          }
        }
      ]
    },
    {
      "name": "vp8CpuUsed",
      "label": "CPU used",
      "description": "Quality/Speed ratio modifier",
      "group": "ADBEVideoCodecGroup",
      "type": "int",
      "flags": [ "slider" ],
      "defaultValue": 1,
      "minValue": 0,
      "maxValue": 16,
      "parameters": {
        "cpu-used": "%d"
      }
    }
  ]
}